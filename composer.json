{
	"name": "vasek-purchart/rabbit-mq-consumer-handler-bundle",
	"description": "Handle messages in RabbitMQ consumers in a safe and effective way",
	"license": "MIT",
	"authors": [
		{
			"name": "Va≈°ek Purchart",
			"email": "me@vasekpurchart.cz",
			"homepage": "http://vasekpurchart.cz"
		}
	],
	"require": {
		"php": "~7.4 || ~8.0",
		"consistence-community/consistence": "~2.1",
		"doctrine/orm": "~2.6",
		"emag-tech-labs/rabbitmq-bundle": "~2.2",
		"psr/log": "~1.0",
		"symfony/config": "~4.3|~5.0",
		"symfony/dependency-injection": "~4.3|~5.0",
		"symfony/http-kernel": "~4.3|~5.0",
		"symfony/yaml": "~4.3|~5.0"
	},
	"require-dev": {
		"consistence-community/coding-standard": "3.11.0",
		"matthiasnoback/symfony-dependency-injection-test": "4.2.1",
		"php-parallel-lint/php-parallel-lint": "1.2.0",
		"phing/phing": "2.16.4",
		"phpunit/phpunit": "9.5.2"
	},
	"autoload": {
		"psr-4": {
			"VasekPurchart\\RabbitMqConsumerHandlerBundle\\": "src"
		},
		"classmap": [ "src" ]
	},
	"autoload-dev": {
		"psr-4": {
			"VasekPurchart\\RabbitMqConsumerHandlerBundle\\": "tests"
		},
		"classmap": [ "tests" ]
	},
	"config": {
		"bin-dir": "bin"
	}
}
